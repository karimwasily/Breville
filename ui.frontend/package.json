{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -t http://admin:admin@localhost:4502 -w ../ui.apps/src/main/content",
    "storybook": "start-storybook -p 6002",
    "w": "watch 'npm run prod' ./src",
    "stylelint": "npx stylelint '**/*.scss'; exit 0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@adobe/htlengine": "^6.3.5",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.16",
    "@storybook/aem": "0.0.11",
    "@storybook/preset-scss": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "antlr4": "^4.9.1",
    "aemfed": "^0.1.1",
    "aemsync": "^4.0.1",
    "autoprefixer": "^9.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.6",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-force-resolutions": "0.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.35.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "storybook-aem-grid": "^0.1.3",
    "storybook-aem-style-system": "^0.1.7",
    "storybook-aem-wrappers": "^0.3.0",
    "style-loader": "^0.14.1",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@adobe/aem-core-cif-react-components": "^1.7.0",
    "i18next": "^19.5.3",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "jquery": "^3.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0"
  },
  "resolutions": {
    "@adobe/htlengine": "6.3.5"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "@storybook/aem-cli": {
    "storybookLocation": ".storybook",
    "storybookStoryLocation": "./stories",
    "localPackagePath": ".storybook/aem-library/",
    "appsPath": "../ui.apps/src/main/content/jcr_root/apps",
    "componentPaths": [
      "../ui.apps/src/main/content/jcr_root/apps/breville-brands/components"
    ],
    "storybookAEMStyleSystem": true,
    "storybookAEMGrid": true,
    "storybookAEMPageTemplate": true,
    "storybookAEMConfluence": false,
    "storybookAEMFoundation": false,
    "aemContentDefaultPageResourceType": "breville-brands/components/page",
    "aemContentDefaultPageTemplate": "/conf/breville-brands/settings/wcm/templates/page-content",
    "aemContentDefaultPageContentPath": "/root/container/container",
    "aemStoryHeadingComponentResourceType": "breville-brands/components/title",
    "aemStoryHeadingComponentTitleProperty": "jcr:title",
    "storyRoot": "Components",
    "packageGroup": "breville-brands",
    "packageName": "Breville Storybook Library.zip",
    "aemContentPath": "/content/breville-brands-style-system",
    "createAEMContent": true,
    "libs": [
      "/libs/core/wcm/components"
    ]
  }
}
