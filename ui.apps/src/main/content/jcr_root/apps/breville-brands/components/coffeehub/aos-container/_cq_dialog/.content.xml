<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="AOS Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.container.v1.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_container_v1"
    trackingFeature="core-components:container:v1">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <aos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Animation"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <animation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please configure the required animation"
                                                fieldLabel="Animation"
                                                name="./animation">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <selectfade
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Fade"
                                                        value=""/>
                                                    <fade
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fade"
                                                        value="fade"/>
                                                    <fadeup
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fade Up"
                                                        value="fade-up"/>
                                                    <fadedown
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fade Down"
                                                        value="fade-down"/>
                                                    <fadeleft
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fade Left"
                                                        value="fade-left"/>
                                                    <faderight
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fade Right"
                                                        value="fade-right"/>
                                                    <slideleft
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Slide Left"
                                                        value="slide-left"/>
                                                    <slideright
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Slide Right"
                                                        value="slide-right"/>
                                                </items>
                                            </animation>
                                            <anchorPlacement
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please configure the required anchor placement"
                                                fieldLabel="Anchor Placement"
                                                name="./anchorPlacement">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <selectanchor
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Anchor Placement"
                                                        value=""/>
                                                    <top-bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top Bottom"
                                                        value="top-bottom"/>
                                                    <top-center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top Center"
                                                        value="top-center"/>
                                                    <top-top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top Top"
                                                        value="top-top"/>
                                                    <center-bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Center Bottom"
                                                        value="center-bottom"/>
                                                    <center-center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Center Center"
                                                        value="center-center"/>
                                                    <center-top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Center Top"
                                                        value="center-top"/>
                                                    <bottom-bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bottom Bottom"
                                                        value="bottom-bottom"/>
                                                    <bottom-center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bottom Center"
                                                        value="bottom-center"/>
                                                    <bottom-top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bottom Top"
                                                        value="bottom-top"/>
                                                </items>
                                            </anchorPlacement>
                                            <easing
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="You can choose one of these timing function to animate elements nicely"
                                                fieldLabel="Easing"
                                                name="./easing">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <selecteasing
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Easing"
                                                        value=""/>
                                                    <linear
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Linear"
                                                        value="linear"/>
                                                    <ease
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Ease"
                                                        value="ease"/>
                                                    <ease-in
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Ease In"
                                                        value="ease-in"/>
                                                    <ease-out
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Ease Out"
                                                        value="ease-out"/>
                                                </items>
                                            </easing>
                                            <delay
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delay(Milliseconds)"
                                                name="./delay"/>
                                            <offset
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Offset(pixels)"
                                                name="./offset"/>
                                            <duration
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Duration(Milliseconds)"
                                                name="./duration"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </aos>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
