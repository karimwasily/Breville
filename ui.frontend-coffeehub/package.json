{
  "name": "bvl-coffee-hub-fe",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "scripts": {
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -t http://admin:admin@localhost:4502 -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -t http://admin:admin@localhost:4502 -w ../ui.apps/src/main/content",
    "aem:watch": "cross-env aemfed -t \"http://admin:admin@localhost:4502\" -w ../ui.apps/src/main/content/jcr_root/,../ui.content/src/main/content/jcr_root/ -e \"**/.*.sw?\"",
    "build:watch": "cross-env watch 'npm run prod' ./src",
    "aemfed": "cross-env FORCE_COLOR=1 CI=true concurrently --prefix \"[ {name} ]\"  --names \"AEM Fed, Webpack \" --prefix-colors \"white.bgCyan.bold,white.bgGreen.bold\" \"npm run aem:watch\" \"npm run build:watch\" --kill-others",
    "stylelint": "npx stylelint '**/*.scss'; exit 0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/aos": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "aemfed": "^0.1.1",
    "aemsync": "^4.0.1",
    "autoprefixer": "^9.2.1",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.35.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.14.1",
    "stylelint": "^13.11.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "jquery": "^3.4.1",
    "simple-parallax-js": "^5.6.2",
    "tippy.js": "^6.3.1"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
