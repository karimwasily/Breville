{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "prepare": "cd ../ && husky install && husky set .husky/pre-commit \"cd ./ui.frontend-breville && npm run pre-commit\"",
    "pre-commit": "lint-staged",
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "start:coffee-landing": "cross-env APP_VIEW=CL ENV=development npm run start",
    "start:solution-landing": "cross-env APP_VIEW=SL ENV=development npm run start",
    "start:plp": "cross-env APP_VIEW=PLP ENV=development npm run start",
    "start:compare": "cross-env APP_VIEW=COMPARE ENV=development npm run start",
    "start:header": "cross-env APP_VIEW=HEADER ENV=development yarn start",
    "start:pdp": "cross-env APP_VIEW=PDP ENV=development npm run start",
    "start:search": "cross-env APP_VIEW=SEARCH ENV=development yarn start",
    "start:footer": "cross-env APP_VIEW=FOOTER ENV=development npm run start",
    "start:pdp-archive": "cross-env APP_VIEW=PDPARCHIVE ENV=development npm run start",
    "start:banners": "cross-env APP_VIEW=BANNERS ENV=development npm run start",
    "start:ar": "cross-env APP_VIEW=AR ENV=development npm run start",
    "start:bundle": "cross-env APP_VIEW=BUNDLE ENV=development yarn start",
    "start:cart": "cross-env APP_VIEW=CART ENV=development npm run start",
    "start:checkout": "cross-env APP_VIEW=CHECKOUT ENV=development npm run start",
    "start:experience": "cross-env APP_VIEW=EXPERIENCE ENV=development npm run start",
    "start:order": "cross-env APP_VIEW=ORDER ENV=development npm run start",
    "start:beanz": "cross-env APP_VIEW=BEANZ ENV=development npm run start",
    "start:mybreville": "cross-env APP_VIEW=MYBREVILLE ENV=development npm run start",
    "start:mybreville-support": "cross-env APP_VIEW=MYBREVILLE_SUPPORT ENV=development npm run start",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -t http://admin:admin@localhost:4502 -w ../ui.apps/src/main/content",
    "storybook": "start-storybook -p 6002",
    "w": "watch 'npm run prod' ./src",
    "aem:watch": "cross-env aemfed -t http://admin:admin@localhost:4502 -w ../ui.apps/src/main/content/jcr_root/,../ui.content/src/main/content/jcr_root/ -e \"**/.*.sw?\"",
    "build:watch": "cross-env watch 'npm run prod' ./src",
    "aemfed": "cross-env FORCE_COLOR=1 CI=true concurrently --prefix \"[ {name} ]\"  --names \"AEM Fed, Webpack \" --prefix-colors \"white.bgCyan.bold,white.bgGreen.bold\" \"npm run aem:watch\" \"npm run build:watch\" --kill-others",
    "stylelint": "npx stylelint '**/*.scss'; exit 0"
  },
  "devDependencies": {
    "@adobe/htlengine": "^6.3.5",
    "@babel/core": "^7.10.4",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.16",
    "@splidejs/react-splide": "0.4.4",
    "@storybook/aem": "0.0.11",
    "@storybook/preset-scss": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "aemfed": "^0.1.1",
    "aemsync": "^4.0.1",
    "antlr4": "^4.9.1",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.12.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-force-resolutions": "0.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react-svg-loader": "^3.0.3",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.35.2",
    "sass-loader": "^10.0.3",
    "source-map-loader": "^0.2.4",
    "storybook-aem-grid": "^0.1.3",
    "storybook-aem-style-system": "^0.1.7",
    "storybook-aem-wrappers": "^0.3.0",
    "style-loader": "^0.14.1",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-merge": "^4.2.1"
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "dependencies": {
    "@adyen/adyen-web": "^4.7.2",
    "@auth0/auth0-react": "^1.6.0",
    "classnames": "^2.3.1",
    "i18next": "^19.5.3",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "instantsearch.css": "^7.4.5",
    "jquery": "^3.6.0",
    "lodash.get": "^4.4.2",
    "lodash.orderby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "normalizr": "^3.6.1",
    "query-string": "^7.0.1",
    "rc-collapse": "^3.1.1",
    "react-i18next": "^11.7.0",
    "react-instantsearch-dom": "^6.11.0",
    "react-modal": "^3.13.1",
    "react-page-visibility": "^6.4.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.4",
    "react-slick": "^0.28.1",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rheostat": "^4.1.1",
    "xps-react": "*",
    "xps-utils": "*"
  },
  "resolutions": {
    "@adobe/htlengine": "6.3.5"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "@storybook/aem-cli": {
    "storybookLocation": ".storybook",
    "storybookStoryLocation": "./stories",
    "localPackagePath": ".storybook/aem-library/",
    "appsPath": "../ui.apps/src/main/content/jcr_root/apps",
    "componentPaths": [
      "../ui.apps/src/main/content/jcr_root/apps/breville-brands/components"
    ],
    "storybookAEMStyleSystem": true,
    "storybookAEMGrid": true,
    "storybookAEMPageTemplate": true,
    "storybookAEMConfluence": false,
    "storybookAEMFoundation": false,
    "aemContentDefaultPageResourceType": "breville-brands/components/page",
    "aemContentDefaultPageTemplate": "/conf/breville-brands/settings/wcm/templates/page-content",
    "aemContentDefaultPageContentPath": "/root/container/container",
    "aemStoryHeadingComponentResourceType": "breville-brands/components/title",
    "aemStoryHeadingComponentTitleProperty": "jcr:title",
    "storyRoot": "Components",
    "packageGroup": "breville-brands",
    "packageName": "Breville Storybook Library.zip",
    "aemContentPath": "/content/breville-brands-style-system",
    "createAEMContent": true,
    "libs": [
      "/libs/core/wcm/components"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.9.0",
    "react-router-dom": "^5.2.0"
  }
}
